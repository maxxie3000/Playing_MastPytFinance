    "#Calculate risk neutral propability Q\n",
    "import math \n",
    "\n",
    "r = 0.05\n",
    "T = 2\n",
    "t = T / 2\n",
    "u = 1.3\n",
    "d = 0.9\n",
    "#Calculate probability Q\n",
    "q = (math.exp(r*t)-d) / (u-d)\n",
    "\n",
    "print(f'Q is {q}')"
